{
  "name": "frontify/color-api",
  "description": "A little Color API",
  "type": "project",
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5",
    "phpmd/phpmd": "^2.8",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "license": "Proprietary",
  "require": {
    "sunrise/http-server-request": "^1.1",
    "ext-json": "*",
    "php": "^7.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Frontify\\ColorApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Frontify\\Tests\\": "tests/"
    }
  }
}
